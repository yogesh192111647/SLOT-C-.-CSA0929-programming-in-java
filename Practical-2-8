public interface Tree {
    void fruits(String... fruits);
    void leaves(String... leaves);
    void flowers(String... flowers);
}

public class Branch1 implements Tree {
    private String[] fruits;
    private String[] leaves;
    private String[] flowers;

    public Branch1(String... fruits) {
        this.fruits = fruits;
    }

    @Override
    public void fruits(String... fruits) {
        this.fruits = fruits;
    }

    @Override
    public void leaves(String... leaves) {
        this.leaves = leaves;
    }

    @Override
    public void flowers(String... flowers) {
        this.flowers = flowers;
    }

    public String[] getFruits() {
        return fruits;
    }

    public String[] getLeaves() {
        return leaves;
    }

    public String[] getFlowers() {
        return flowers;
    }
}

public class Branch2 implements Tree {
    private String[] fruits;
    private String[] leaves;
    private String[] flowers;

    public Branch2(String... fruits) {
        this.fruits = fruits;
    }

    @Override
    public void fruits(String... fruits) {
        this.fruits = fruits;
    }

    @Override
    public void leaves(String... leaves) {
        this.leaves = leaves;
    }

    @Override
    public void flowers(String... flowers) {
        this.flowers = flowers;
    }

    public String[] getFruits() {
        return fruits;
    }

    public String[] getLeaves() {
        return leaves;
    }

    public String[] getFlowers() {
        return flowers;
    }
}
